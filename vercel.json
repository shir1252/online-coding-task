{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/server.js",
        "methods": ["GET"],
        "headers": { "cache-control": "s-maxage=60" },
        "continue": true,
        "status": 200,
        "timeout": 30 
      }
    ]
  }
  